        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 50, 100))
    }
    welcomeGradient.Rotation = 45
    welcomeGradient.Parent = welcomeCard
    
    local welcomeTitle = Instance.new("TextLabel")
    welcomeTitle.Size = UDim2.new(1, -20, 0, 30)
