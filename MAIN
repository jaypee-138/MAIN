    infoTitle.Position = UDim2.new(0, 10, 0, 10)
    infoTitle.BackgroundTransparency = 1
    infoTitle.Font = Enum.Font.GothamBold
    infoTitle.Text = "‚öôÔ∏è UI Information"
    infoTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    infoTitle.TextSize = 14
    infoTitle.TextXAlignment = Enum.TextXAlignment.Left
    infoTitle.Parent = infoCard
    
    local infoText = Instance.new("TextLabel")
    infoText.Size = UDim2.new(1, -20, 0, 70)
    infoText.Position = UDim2.new(0, 10, 0, 40)
    infoText.BackgroundTransparency = 1
    infoText.Font = Enum.Font.Gotham
    infoText.Text = "Version: 4.0 Blue & Black Edition\nCreator: STAN Team\nTheme: Blue & Black Modern\nStatus: All systems operational"
    infoText.TextColor3 = Color3.fromRGB(150, 180, 255)
    infoText.TextSize = 11
    infoText.TextXAlignment = Enum.TextXAlignment.Left
    infoText.TextYAlignment = Enum.TextYAlignment.Top
    infoText.Parent = infoCard
    
    -- Keybinds Card
    local keybindsCard = Instance.new("Frame")
    keybindsCard.Size = UDim2.new(1, -8, 0, 300)
    keybindsCard.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    keybindsCard.BorderSizePixel = 0
    keybindsCard.Parent = scroll
    
    Instance.new("UICorner", keybindsCard).CornerRadius = UDim.new(0, 12)
    
    local keybindsStroke = Instance.new("UIStroke")
    keybindsStroke.Color = Color3.fromRGB(0, 150, 255)
    keybindsStroke.Thickness = 1.5
    keybindsStroke.Parent = keybindsCard
    
    local keybindsTitle = Instance.new("TextLabel")
    keybindsTitle.Size = UDim2.new(1, -20, 0, 30)
    keybindsTitle.Position = UDim2.new(0, 10, 0, 10)
    keybindsTitle.BackgroundTransparency = 1
    keybindsTitle.Font = Enum.Font.GothamBold
    keybindsTitle.Text = "‚å®Ô∏è Keybinds Configuration"
    keybindsTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    keybindsTitle.TextSize = 14
    keybindsTitle.TextXAlignment = Enum.TextXAlignment.Left
    keybindsTitle.Parent = keybindsCard
    
    local keybindsScroll = Instance.new("ScrollingFrame")
    keybindsScroll.Size = UDim2.new(1, -20, 1, -50)
    keybindsScroll.Position = UDim2.new(0, 10, 0, 40)
    keybindsScroll.BackgroundTransparency = 1
    keybindsScroll.BorderSizePixel = 0
    keybindsScroll.ScrollBarThickness = 4
    keybindsScroll.ScrollBarImageColor3 = Color3.fromRGB(0, 150, 255)
    keybindsScroll.Parent = keybindsCard
    
    local keybindsLayout = Instance.new("UIListLayout")
    keybindsLayout.Padding = UDim.new(0, 8)
    keybindsLayout.Parent = keybindsScroll
    
    local function createKeybind(name, icon, currentKey)
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, -8, 0, 40)
        frame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
        frame.BorderSizePixel = 0
        frame.Parent = keybindsScroll
        
        Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 8)
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(0.6, 0, 1, 0)
        nameLabel.Position = UDim2.new(0, 10, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.Text = icon .. " " .. name
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.TextSize = 12
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = frame
        
        local keyButton = Instance.new("TextButton")
        keyButton.Size = UDim2.new(0, 80, 0, 28)
        keyButton.Position = UDim2.new(1, -88, 0.5, -14)
        keyButton.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
        keyButton.BorderSizePixel = 0
        keyButton.Font = Enum.Font.GothamBold
        keyButton.Text = currentKey.Name
        keyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        keyButton.TextSize = 11
        keyButton.Parent = frame
        
        Instance.new("UICorner", keyButton).CornerRadius = UDim.new(0, 6)
        
        local listening = false
        
        keyButton.MouseButton1Click:Connect(function()
            if listening then return end
            listening = true
            keyButton.Text = "..."
            keyButton.BackgroundColor3 = Color3.fromRGB(255, 200, 0)
            
            local connection
            connection = UIS.InputBegan:Connect(function(input, gameProcessed)
                if input.UserInputType == Enum.UserInputType.Keyboard then
                    keybinds[name] = input.KeyCode
                    keyButton.Text = input.KeyCode.Name
                    keyButton.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
                    listening = false
                    connection:Disconnect()
                    
                    game:GetService("StarterGui"):SetCore("SendNotification", {
                        Title = "Keybind Updated!";
                        Text = name .. " ‚Üí " .. input.KeyCode.Name;
                        Duration = 2;
                    })
                end
            end)
        end)
        
        return frame
    end
    
    createKeybind("Fly", "‚úàÔ∏è", keybinds.Fly)
    createKeybind("Speed", "üèÉ", keybinds.Speed)
    createKeybind("Jump", "ü¶ò", keybinds.Jump)
    createKeybind("Noclip", "üëª", keybinds.Noclip)
    createKeybind("ESP", "üëÅÔ∏è", keybinds.ESP)
    createKeybind("Wallbang", "üß±", keybinds.Wallbang)
    
    keybindsScroll.CanvasSize = UDim2.new(0, 0, 0, keybindsLayout.AbsoluteContentSize.Y + 10)
    
    -- Destroy Button
    local destroyBtn = Instance.new("TextButton")
    destroyBtn.Size = UDim2.new(1, -8, 0, 45)
    destroyBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 80)
    destroyBtn.BorderSizePixel = 0
    destroyBtn.Font = Enum.Font.GothamBold
    destroyBtn.Text = "üóëÔ∏è Destroy UI"
    destroyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    destroyBtn.TextSize = 13
    destroyBtn.Parent = scroll
    
    Instance.new("UICorner", destroyBtn).CornerRadius = UDim.new(0, 10)
    
    destroyBtn.MouseButton1Click:Connect(function()
        TweenService:Create(main, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -350, 1.5, 0)}):Play()
        task.wait(0.3)
        gui:Destroy()
    end)
    
    spawn(function()
        while scroll.Parent do
            scroll.CanvasSize = UDim2.new(0, 0, 0, settingsLayout.AbsoluteContentSize.Y + 10)
            task.wait(0.1)
        end
    end)
end)

-- EXTRAS TAB
extrasTab.MouseButton1Click:Connect(function()
    clearContent()
    
    for _, tab in pairs(tabs:GetChildren()) do
        if tab:IsA("TextButton") then
            tab.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
            tab.TextColor3 = Color3.fromRGB(150, 150, 170)
            local stroke = tab:FindFirstChildOfClass("UIStroke")
            if stroke then stroke.Transparency = 1 end
        end
    end
    
    extrasTab.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    extrasTab.TextColor3 = Color3.fromRGB(255, 255, 255)
    extrasStroke.Transparency = 0
    extrasStroke.Color = Color3.fromRGB(0, 180, 255)
    
    local extrasContainer = Instance.new("Frame")
    extrasContainer.Size = UDim2.new(1, -20, 1, -20)
    extrasContainer.Position = UDim2.new(0, 10, 0, 10)
    extrasContainer.BackgroundTransparency = 1
    extrasContainer.Parent = content
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBold
    title.Text = "‚ú® Coming Soon..."
    title.TextColor3 = Color3.fromRGB(0, 180, 255)
    title.TextSize = 20
    title.Parent = extrasContainer
    
    local desc = Instance.new("TextLabel")
    desc.Size = UDim2.new(1, 0, 0, 60)
    desc.Position = UDim2.new(0, 0, 0, 50)
    desc.BackgroundTransparency = 1
    desc.Font = Enum.Font.Gotham
    desc.Text = "More features will be added in future updates!\n\nStay tuned for awesome additions."
    desc.TextColor3 = Color3.fromRGB(150, 150, 170)
    desc.TextSize = 12
    desc.TextYAlignment = Enum.TextYAlignment.Top
    desc.Parent = extrasContainer
end)

-- Load Home Tab
homeTab.MouseButton1Click:Fire()

-- Entrance Animation
main.Position = UDim2.new(0.5, -350, -1, 0)
TweenService:Create(main, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.5, -350, 0.5, -225)
}):Play()

-- Notification
pcall(function()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "STAN UI v4.0";
        Text = "Blue & Black Edition loaded!";
        Duration = 3;
    })
end)

