-- KEY VERIFICATION
verifyBtn.MouseButton1Click:Connect(function()
    local enteredKey = keyInput.Text
    
    if enteredKey == "" then
        statusLabel.Text = "‚ùå Error\n\nPlease enter a key!"
        statusLabel.BackgroundColor3 = Color3.fromRGB(239, 68, 68)
        statusLabel.BackgroundTransparency = 0.9
        return
    end
    
    verifyBtn.Text = "‚è≥ Verifying..."
    verifyBtn.BackgroundColor3 = Color3.fromRGB(251, 191, 36)
    statusLabel.Text = "üîÑ Checking...\n\nPlease wait..."
    statusLabel.BackgroundColor3 = Color3.fromRGB(251, 191, 36)
    statusLabel.BackgroundTransparency = 0.9
    
    task.wait(0.8)
    
    if enteredKey == YOUR_SECRET_KEY then
        verifyBtn.Text = "‚úì Success!"
        verifyBtn.BackgroundColor3 = Color3.fromRGB(34, 197, 94)
        statusLabel.Text = "‚úÖ Access Granted!\n\nLoading UI..."
        statusLabel.BackgroundColor3 = Color3.fromRGB(34, 197, 94)
        statusLabel.BackgroundTransparency = 0.9
        
        task.wait(0.6)
        
        TweenService:Create(keyFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Position = UDim2.new(0.5, -210, -1, 0)
        }):Play()
        
        task.wait(0.3)
        keyFrame:Destroy()
        
        mainFrame.Visible = true
        mainFrame.Position = UDim2.new(0.5, -325, -1, 0)
        TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Position = UDim2.new(0.5, -325, 0.5, -205)
        }):Play()
        
        pcall(function()
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "STAN UI v3.0";
                Text = "Successfully loaded!";
                Duration = 3;
            })
        end)
    else
        verifyBtn.Text = "‚úó Invalid Key!"
        verifyBtn.BackgroundColor3 = Color3.fromRGB(239, 68, 68)
        statusLabel.Text = "‚ùå Wrong Key!\n\nPlease try again"
        statusLabel.BackgroundColor3 = Color3.fromRGB(239, 68, 68)
        statusLabel.BackgroundTransparency = 0.9
        keyInput.Text = ""
        
        task.wait(1.5)
        
        verifyBtn.Text = "üîì Verify Key"
        verifyBtn.BackgroundColor3 = Color3.fromRGB(147, 51, 234)
        statusLabel.Text = "üîí Access Required\n\nEnter key to continue"
        statusLabel.BackgroundColor3 = Color3.fromRGB(59, 130, 246)
        statusLabel.BackgroundTransparency = 0.9
    end
end)

minBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
    minFrame.Visible = true
end)

minFrame.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    minFrame.Visible = false
end)

closeBtn.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

-- DRAG
local dragging, dragInput, dragStart, startPos

topBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
    end
end)

UIS.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

UIS.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(
            startPos.X.Scale, startPos.X.Offset + delta.X,
            startPos.Y.Scale, startPos.Y.Offset + delta.Y
        )
    end
end)

-- TABS
local function clearContent()
    for _, v in pairs(content:GetChildren()) do
        if v:IsA("GuiObject") then
            v:Destroy()
        end
    end
end

local function makeTab(name, icon, order)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, 0, 0, 38)
    btn.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
    btn.BorderSizePixel = 0
    btn.Font = Enum.Font.GothamBold
    btn.Text = icon .. " " .. name
    btn.TextColor3 = Color3.fromRGB(180, 180, 200)
    btn.TextSize = 12
    btn.LayoutOrder = order
    btn.Parent = tabs
    
    local btnCorner2 = Instance.new("UICorner")
    btnCorner2.CornerRadius = UDim.new(0, 7)
    btnCorner2.Parent = btn
    
    return btn
end

