    welcome.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    welcome.BorderSizePixel = 0
    welcome.Parent = content
    
    Instance.new("UICorner", welcome).CornerRadius = UDim.new(0, 12)
    
    local welcomeStroke = Instance.new("UIStroke")
    welcomeStroke.Color = Color3.fromRGB(0, 150, 255)
    welcomeStroke.Thickness = 1.5
    welcomeStroke.Parent = welcome
    
